<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="StockOpPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>org.jakc.stockop.entity.Site</class>
    <class>org.jakc.stockop.entity.Transtockdetail</class>
    <class>org.jakc.stockop.entity.Employeetype</class>
    <class>org.jakc.stockop.entity.Employee</class>
    <class>org.jakc.stockop.entity.Gondola</class>
    <class>org.jakc.stockop.entity.Transstock</class>
    <class>org.jakc.stockop.entity.Periode</class>
    <class>org.jakc.stockop.entity.Roles</class>
    <class>org.jakc.stockop.entity.Product</class>
    <class>org.jakc.stockop.entity.Stock</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <validation-mode>NONE</validation-mode>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/opnam"/>
      <property name="javax.persistence.jdbc.password" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
    </properties>
  </persistence-unit>
</persistence>
